{
  "plugins": [
    "syntax-dynamic-import",
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1"
        }
      }
    ],
    [
      "modern-browsers",
      {
        "edge": false
      }
    ]
  ]
}
